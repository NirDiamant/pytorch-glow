{
  "profile": "celebahq_256x256_5bit",
  "general": {
    "verbose": false,
    "root_dir": ".",
    "restore_dir": "snapshot",
    "log_dir": "snapshot",
    "data_dir": "snapshot"
  },
  "dataset": {
    "problem": "celeba",
    "n_classes": 1,
    "argument": "standard",
    "fmap": 1,
    "pmap": 1
  },
  "optim": {
    "epochs": 1000000,
    "n_train": 50000,
    "n_test": -1,
    "n_sample": 1,
    "interval_valid": 50,
    "interval_sample": 50,
    "n_batch_train": 16,
    "n_batch_test": 50,
    "n_batch_init": 256,
    "optimizer": "adamax",
    "optimizer_args":{
      "lr": 1e-3,
      "betas": [0.9, 0.99],
      "eps": 1e-8,
      "weight_decay": 0
    },
    "lr_scheduler": "noam",
    "lr_shceduler_args": {
      "warmup_steps": 4000,
    },
    "gradient_checkpointing": true
  },
  "model": {
    "image_shape": [
      64,
      64,
      3
    ],
    "anchor_size": 32,
    "hidden_channels": 512,
    "actnorm_scale": 1.0,
    "K": 32,
    "L": 3,
    "weight_y": 0.0,
    "n_bits_x": 8
  },
  "ablation": {
    "learn_top": false,
    "y_cond": false,
    "lu_decomposition": false,
    "seed": 0,
    "flow_permutation": "invconv",
    "flow_coupling": "additive"
  },
  "device": {
    "graph": [
      "cuda:0",
      "cuda:1"
    ],
    "data":[
      "cpu"
    ]
  }
}
